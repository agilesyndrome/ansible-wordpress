export WORDPRESS_CONFIG=$(secret ${item})
export DB_NAME=$(echo ${WORDPRESS_CONFIG} | jq -r '.dbname')
export DB_USER=$(echo ${WORDPRESS_CONFIG} | jq -r '.dbuser')
export DB_PASS=$(echo ${WORDPRESS_CONFIG} | jq -r '.dbpass')
export DB_HOST=$(echo ${WORDPRESS_CONFIG} | jq -r '.dbhost')
export DB_PREFIX=$(echo ${WORDPRESS_CONFIG} | jq -r '.dbprefix')
export ADMIN_NAME=$(echo ${WORDPRESS_CONFIG} | jq -r '.admin_name')
export ADMIN_EMAIL=$(echo ${WORDPRESS_CONFIG} | jq -r '.admin_email')
export ADMIN_PASSWORD=$(echo ${WORDPRESS_CONFIG} | jq -r '.admin_password')
export WP_SITE=$(echo ${WORDPRESS_CONFIG} | jq -r '.site')